public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    if (passedExam = true && passedAssignments = true) {
        return true;
    } if else (passedExam = true && passedProject = true) {
        return true;
    } if else (passedAssignments = true && passedProject = true) {
        return true;
    } else {
        return false;
    }
}